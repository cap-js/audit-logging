{
  "name": "@cap-js/audit-logging",
  "version": "0.0.1",
  "description": "CAP cds-plugin providing audit logging re personal data out-of-the-box.",
  "repository": "cap-js/audit-logging",
  "author": "SAP SE (https://www.sap.com)",
  "homepage": "https://cap.cloud.sap/",
  "license": "SEE LICENSE IN LICENSE",
  "main": "cds-plugin.js",
  "files": [
    "lib",
    "srv"
  ],
  "peerDependencies": {
    "@sap/cds": "*"
  },
  "devDependencies": {
    "eslint": "^8",
    "@sap/audit-logging": "^5.7.0"
  },
  "cds": {
    "requires": {
      "audit-log": true,
      "kinds": {
        "audit-log": {
          "_model": "@cap-js/audit-logging",
          "[development]": {
            "kind": "audit-log-to-console"
          },
          "[production]": {
            "kind": "audit-log-to-library"
          }
        },
        "audit-log-to-console": {
          "impl": "@cap-js/audit-logging/srv/log2console",
          "outbox": false
        },
        "audit-log-to-library": {
          "impl": "@cap-js/audit-logging/srv/log2library",
          "outbox": true,
          "_credentials": {
            "logToConsole": true
          }
        }
      }
    }
  }
}
