{
  "name": "@cap-js/audit-logging",
  "version": "0.3.0",
  "description": "CDS plugin providing integration to the SAP BTP Audit Logging Service as well as out-of-the-box personal data-related audit logging based on annotations.",
  "repository": "cap-js/audit-logging",
  "author": "SAP SE (https://www.sap.com)",
  "homepage": "https://cap.cloud.sap/",
  "license": "SEE LICENSE IN LICENSE",
  "main": "cds-plugin.js",
  "files": [
    "lib",
    "srv"
  ],
  "scripts": {
    "lint": "npx eslint .",
    "test": "npx jest --silent"
  },
  "peerDependencies": {
    "@sap/cds": "^7"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1",
    "axios": "^1.4",
    "better-sqlite3": "^8.5",
    "eslint": "^8.48",
    "express": "^4.18",
    "jest": "^29.6"
  },
  "cds": {
    "requires": {
      "audit-log": true,
      "kinds": {
        "audit-log": {
          "handle": [
            "READ",
            "WRITE"
          ],
          "[development]": {
            "kind": "audit-log-to-console"
          },
          "[production]": {
            "kind": "audit-log-to-restv2"
          }
        },
        "audit-log-to-console": {
          "impl": "@cap-js/audit-logging/srv/log2console",
          "outbox": false
        },
        "audit-log-to-restv2": {
          "impl": "@cap-js/audit-logging/srv/log2restv2",
          "vcap": {
            "label": "auditlog"
          },
          "outbox": true
        }
      }
    }
  }
}
